#include <iostream>
#include <conio.h>

using namespace std;
int main ()
 {
      int x = 31;
      cout << "Counting... "; 
      cout << endl;
      
      for( int i = 1; i < x; i++ )
      {
        if(i > 10) 
		  {
        i = i+1;
      }
          
		  else
		  {
        i = i;
      }
      cout << i << ","<<' '; 
          
        if (x==30) 
		  {
			cout << "30";
			break;
		  }
      }
      
_getch();      
return 0;
}
